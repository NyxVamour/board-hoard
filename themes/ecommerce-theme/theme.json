{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"custom": false,
			"customGradient": false,
			"customDuotone": false,
			"palette": [
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "primary",
					"color": "#AD3518",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00A3FB",
					"name": "Secondary"
				},
				{
					"slug": "secondary-accent",
					"color": "#037FC3",
					"name": "Secondary Accent"
				},
				{
					"slug": "tertiary",
					"color": "#56423C",
					"name": "Tertiary"
				},
				{
					"slug": "tertiary-accent",
					"color": "#281412",
					"name": "Tertiary Accent"
				},
				{
					"slug": "quarternary",
					"color": "#C59A43",
					"name": "Quarternary"
				}
			]
		},
		"layout": {
			"contentSize": "1100px",
			"wideSize": "1800px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "Kanit",
					"slug": "kanit",
					"fontFamily": "Kanit, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Kanit",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/kanit/Kanit-ExtraBold.ttf" ]
						}
					]
				},
				{
					"name": "Outfit",
					"slug": "outfit",
					"fontFamily": "Outfit, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Outfit",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Outfit-VariableFont_wght.ttf" ]
						}
					]
				},
				{
					"name": "Cairo",
					"slug": "cairo",
					"fontFamily": "Cairo, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Cairo",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Cairo-VariableFont_slnt,wght.ttf" ]
						}
					]
				},
				{
					"name": "Bangers",
					"slug": "Bangers",
					"fontFamily": "Bangers, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Bangers",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Bangers-Regular.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "extra-small",
					"size": "0.875rem",
					"name": "Extra small",
					"fluid": false
				},
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large",
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2.75rem",
					"name": "XL",
					"fluid": {
						"min": "2rem",
						"max": "2.75rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3.5rem",
					"name": "XXL",
					"fluid": {
						"min": "3rem",
						"max": "3.75rem"
					}
				}
			]
		},
		"shadow": {
			"presets": [
				{
					"name": "Straight Shallow",
					"slug": "straight-shallow",
					"shadow": "0px 4px 4px rgba(0, 0, 0, 0.25)"
				},
				{
					"name": "Natural Shallow",
					"slug": "natural-shallow",
					"shadow": "4px 4px 4px rgba(0, 0, 0, 0.25)"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--cairo)",
			"lineHeight": "1.5"
		},
		"color": {
			"text": "var(--wp--preset--color--tertiary-accent)"
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--outfit)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "800"
				}
			},
			"core/post-date": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"dimensions": {
					"marginTop": "0px"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "0px"
				},
				"spacing": {
					"padding": {
						"top": "0px",
						"right": "1rem",
						"bottom": "0px",
						"left": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "800"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary-accent)"
					}
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "single-posts",
			"title": "Single Posts",
			"postTypes": [
				"post"
			]
		}
	]
}
