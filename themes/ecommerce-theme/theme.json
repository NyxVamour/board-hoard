{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"custom": false,
			"customGradient": false,
			"customDuotone": false,
			"palette": [
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "gray-100",
					"color": "#F4F4F4",
					"name": "Gray 100"
				},
				{
					"slug": "primary",
					"color": "#AD3518",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00A3FB",
					"name": "Secondary"
				},
				{
					"slug": "secondary-accent",
					"color": "#037FC3",
					"name": "Secondary Accent"
				},
				{
					"slug": "tertiary",
					"color": "#56423C",
					"name": "Tertiary"
				},
				{
					"slug": "tertiary-accent",
					"color": "#281412",
					"name": "Tertiary Accent"
				},
				{
					"slug": "quarternary",
					"color": "#C59A43",
					"name": "Quarternary"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1800px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"spacingSizes": [
				{
					"name": "XXS",
					"size": "0.75rem",
					"slug": "xxs"
				},
				{
					"name": "XS",
					"size": "clamp(1rem, 0.934rem + 0.351vw, 1.25rem)",
					"slug": "xs"
				},
				{
					"name": "S",
					"size": "clamp(1.25rem, 1.184rem + 0.351vw, 1.5rem)",
					"slug": "s"
				},
				{
					"name": "M",
					"size": "clamp(2rem, 1.868rem + 0.702vw, 2.5rem)",
					"slug": "m"
				},
				{
					"name": "L",
					"size": "clamp(2.5rem, 2.237rem + 1.404vw, 3.5rem)",
					"slug": "l"
				},
				{
					"name": "XL",
					"size": "clamp(3.75rem, 3.553rem + 1.053vw, 4.5rem)",
					"slug": "xl"
				},
				{
					"name": "XXL",
					"size": "clamp(5rem, 4.722rem + 1.053vw, 6rem)",
					"slug": "xxl"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"name": "Kanit",
					"slug": "kanit",
					"fontFamily": "Kanit, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Kanit",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/kanit/Kanit-ExtraBold.ttf" ]
						}
					]
				},
				{
					"name": "Outfit",
					"slug": "outfit",
					"fontFamily": "Outfit, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Outfit",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Outfit-VariableFont_wght.ttf" ]
						}
					]
				},
				{
					"name": "Cairo",
					"slug": "cairo",
					"fontFamily": "Cairo, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Cairo",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Cairo-VariableFont_slnt,wght.ttf" ]
						}
					]
				},
				{
					"name": "Bangers",
					"slug": "Bangers",
					"fontFamily": "Bangers, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Bangers",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Bangers-Regular.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "xx-small",
					"size": "1rem",
					"name": "XX small",
					"fluid": false
				},
				{
					"slug": "xs-small",
					"size": "1.1rem",
					"name": "XS Small",
					"fluid": {
						"min": "1.1rem",
						"max": "1.25rem"
					}
				},
				{
					"slug": "small",
					"size": "1.25rem",
					"name": "Small",
					"fluid": {
						"min": "1.25rem",
						"max": "1.5rem"
					}
				},
				{
					"slug": "medium",
					"size": "1.5rem",
					"name": "Medium",
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					}
				},
				{
					"slug": "large",
					"size": "2rem",
					"name": "Large",
					"fluid": {
						"min": "2rem",
						"max": "2.25rem"
					}
				},
				{
					"slug": "x-large",
					"size": "2.5rem",
					"name": "XL",
					"fluid": {
						"min": "2rem",
						"max": "3rem"
					}
				},
				{
					"slug": "xx-large",
					"size": "3.5rem",
					"name": "XXL",
					"fluid": {
						"min": "3rem",
						"max": "4rem"
					}
				}
			]
		},
		"shadow": {
			"presets": [
				{
					"name": "Straight Shallow",
					"slug": "straight-shallow",
					"shadow": "0px 4px 4px rgba(0, 0, 0, 0.25)"
				},
				{
					"name": "Natural Shallow",
					"slug": "natural-shallow",
					"shadow": "4px 4px 4px rgba(0, 0, 0, 0.25)"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--cairo)",
			"lineHeight": "1.5",
			"fontSize": "var(--wp--preset--font-size--xs-small)",
			"fontWeight": "600"
		},
		"color": {
			"text": "var(--wp--preset--color--tertiary-accent)"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "0px",
						"bottom": "0px"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--outfit)",
					"fontWeight": "800"
				}
			},
			"core/post-date": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--xx-small)",
					"fontWeight": "600"
				},
				"dimensions": {
					"marginTop": "0px"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"dimensions": {
					"marginTop": "0px"
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--xx-small)",
					"fontWeight": "600"
				},
				"dimensions": {
					"marginTop": "0px"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--xs-small)"
				}
			},
			"core/post-content": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "800",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.2"
				}
			},
			"link": {
				"color": {
					"text": "inherit"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "0px"
				},
				"spacing": {
					"padding": {
						"top": "0px",
						"right": "1rem",
						"bottom": "0px",
						"left": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--cairo)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "800"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary-accent)"
					}
				}
			}
		}
	}
}
